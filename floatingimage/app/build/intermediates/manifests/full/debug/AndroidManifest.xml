<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dk.nindroid.rss"
    android:installLocation="auto"
    android:versionCode="119"
    android:versionName="3.4.23" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="11" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.BIND_WALLPAPER" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-feature
        android:name="android.hardware.touchscreen"
        android:required="false" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:hardwareAccelerated="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:largeHeap="true" >
        <activity
            android:name="dk.nindroid.rss.ShowStreams"
            android:screenOrientation="nosensor" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="dk.nindroid.rss.menu.Settings" />
        <activity android:name="dk.nindroid.rss.ClearCache" />
        <activity android:name="dk.nindroid.rss.settings.ManageFeeds" />
        <activity android:name="dk.nindroid.rss.settings.DirectoryBrowser" />
        <activity android:name="dk.nindroid.rss.settings.SourceSelectorFragmentActivity" />
        <activity android:name="dk.nindroid.rss.settings.FlickrBrowser$AlbumActivity" />
        <activity android:name="dk.nindroid.rss.settings.PicasaBrowser" />
        <activity android:name="dk.nindroid.rss.parser.facebook.FacebookAuth" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="floatingimage" />
            </intent-filter>
        </activity>
        <activity android:name="dk.nindroid.rss.settings.FacebookBrowser$SubActivity" >
        </activity>
        <activity android:name="dk.nindroid.rss.parser.facebook.WebAuth" />
        <activity android:name="dk.nindroid.rss.parser.facebook.FacebookAlbumBrowser" />
        <activity android:name="dk.nindroid.rss.parser.facebook.FacebookFriendsBrowser" />
        <activity android:name="dk.nindroid.rss.parser.facebook.FacebookFriendView" />
        <activity android:name="dk.nindroid.rss.flickr.WebAuth" />
        <activity android:name="dk.nindroid.rss.parser.flickr.FlickrAlbumBrowser" />
        <activity android:name="dk.nindroid.rss.parser.picasa.PicasaWebAuth" />
        <activity android:name="dk.nindroid.rss.parser.picasa.PicasaAlbumBrowser" />
        <activity android:name="dk.nindroid.rss.settings.PicasaBrowser$UserActivity" />
        <activity
            android:name="dk.nindroid.rss.settings.About"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="dk.nindroid.rss.menu.WallpaperSettings"
            android:exported="true" />
        <activity android:name="dk.nindroid.rss.menu.ApplicationFeedManagerLauncher" />
        <activity android:name="dk.nindroid.rss.menu.WallpaperFeedManagerLauncher" />

        <service
            android:name="dk.nindroid.rss.Wallpaper"
            android:label="Floating Image"
            android:permission="android.permission.BIND_WALLPAPER" >
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService" />
            </intent-filter>

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/wallpaper" />
        </service>

        <activity android:name="dk.nindroid.rss.settings.PhotobucketBrowser" />
        <activity android:name="dk.nindroid.rss.settings.PhotobucketBrowser$ShowUserActivity" />
        <activity android:name="dk.nindroid.rss.settings.SourceSelector$SourceActivity" />
        <activity android:name="dk.nindroid.rss.settings.PicasaBrowser$AlbumActivity" />
        <activity android:name="dk.nindroid.rss.parser.facebook.FacebookFriendsBrowser$SubActivity" />
        <activity android:name="dk.nindroid.rss.parser.facebook.FacebookFriendView$SubActivity" />
        <activity android:name="dk.nindroid.rss.menu.LiveWallpaperLauncher" />
        <activity android:name="dk.nindroid.rss.parser.picasa.PicasaUserView$AlbumActivity" />
        <activity android:name="dk.nindroid.rss.settings.FeedSettings" />
        <activity
            android:name="dk.nindroid.rss.GalleryActivity"
            android:icon="@drawable/icon_gallery"
            android:label="Floating Gallery"
            android:taskAffinity=".GalleryActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="dk.nindroid.rss.menu.GallerySettings" />
        <activity android:name="dk.nindroid.rss.menu.GalleryFeedManagerLauncher" />
        <activity
            android:name="dk.nindroid.rss.GalleryImageLauncher"
            android:icon="@drawable/icon_gallery"
            android:label="Floating Gallery"
            android:taskAffinity=".GalleryActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="com.cooliris.media.action.REVIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/*" />
                <data android:scheme="file" />
                <data android:scheme="content" />
            </intent-filter>
        </activity>
        <activity android:name="dk.nindroid.rss.settings.FiveHundredPxBrowser$DiscoverActivity" />
        <activity android:name="dk.nindroid.rss.settings.FiveHundredPxBrowser$CategoryActivity" />
        <activity android:name="dk.nindroid.rss.menu.SaveChooser" />
        <activity android:name="dk.nindroid.rss.DockListener" >
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.DESK_DOCK" />
            <category android:name="android.intent.category.DEFAULT" />
        </activity>

        <receiver
            android:name="dk.nindroid.rss.DockBroadcastReceiver"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
            </intent-filter>
        </receiver>

        <activity android:name="dk.nindroid.rss.settings.ClockSettings" />
        <activity android:name="dk.nindroid.rss.menu.ApplicationClockSettingsLauncher" />
        <activity android:name="dk.nindroid.rss.menu.GalleryClockSettingsLauncher" />
        <activity android:name="dk.nindroid.rss.menu.WallpaperClockSettingsLauncher" />

        <service android:name="org.fourthline.cling.android.AndroidUpnpServiceImpl" />
    </application>

</manifest>